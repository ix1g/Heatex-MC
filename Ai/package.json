{
  "name": "discord-ai-bot",
  "version": "1.0.0",
  "description": "A Discord AI bot using Discord.js v14, Gemini AI, and Tesseract OCR.",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node index.ts",
    "build": "tsc",
    "deploy-commands": "ts-node src/deploy-commands.ts",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@discordjs/rest": "^2.0.1",
    "@google/generative-ai": "^0.2.0",
    "discord-api-types": "^0.37.60",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "tesseract.js": "^5.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}